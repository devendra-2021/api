    .topnav a {
    float: left;
    color: #f2f2f2;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
    font-size: 17px;
    }
    /* Change the color of links on hover */
    .topnav a:hover {
    background-color: #ddd;
    color: black;
    }
    /* Add a color to the active/current link */
    .topnav a.active {
    background-color: #4caf50;
    color: white;
    }
    #out {
    float: right;
    }
